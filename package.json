{
  "name": "merng-dashboard",
  "version": "0.1.0",
  "description": "Exercise create dashboard app with merng stack",
  "main": "dist/index.js",
  "author": "ashal farhan",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.ts --trace-warnings",
    "modules": "yarn && yarn --cwd client",
    "start:client": "yarn --cwd client start",
    "ts-check": "tsc && yarn --cwd client ts",
    "build": "yarn build:server && yarn build:client",
    "build:server": "tsc -p ./",
    "build:client": "yarn --cwd client build",
    "start": "node dist/index.js",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=false npm i -g yarn && yarn modules && yarn build"
  },
  "dependencies": {
    "apollo-server-express": "^2.23.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.10.18",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@typegoose/typegoose": "7.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.41",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
